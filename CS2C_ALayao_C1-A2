var a = 25;
var b = 20;
var c = 12;
var d = "15";
const e = 15;
var g = a + b + c + d + e;
var h = d > e;
var i = d < e;
var j = d >= e;
var k = d <= e;
var l = d === e;

console.log(g);
console.log(h);
console.log(i);
console.log(j);
console.log(k);
console.log(l);

var m = a - b - c - d - e;
var n = (a * c) / e;
var o = (e ** c);
const p = (e ** 3);

console.log(m);
console.log(n);
console.log(o);
console.log(p);